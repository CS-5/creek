{
  "name": "creek",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "npm run next:build && npm run next:export",
    "dev": "dotenv npm run wrangler:dev",
    "next:dev": "next dev",
    "next:build": "next build",
    "next:export": "next export",
    "next:start": "next start",
    "next:lint": "next lint",
    "wrangler:dev": "wrangler pages dev --binding AUTH0_ISSUER=$WORKER_AUTH0_ISSUER --binding WORKER_AUTH0_AUDIENCE=$WORKER_AUTH0_AUDIENCE --binding WORKER_AUTH0_TOKEN=$WORKER_AUTH0_TOKEN --binding WORKER_AUTH0_DOMAIN=$WORKER_AUTH0_DOMAIN -- npm run next:dev"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.9.0",
    "@cfworker/jwt": "^1.12.2",
    "@cloudflare/stream-react": "^1.6.1",
    "@tailwindcss/typography": "^0.5.2",
    "auth0": "^2.40.0",
    "daisyui": "^2.8.0",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.4.0",
    "@types/auth0": "^2.34.14",
    "@types/node": "17.0.21",
    "@types/react": "17.0.40",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "dotenv-cli": "^5.0.0",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "install": "^0.13.0",
    "postcss": "^8.4.8",
    "tailwindcss": "^3.0.23",
    "typescript": "4.6.2",
    "wrangler": "^0.0.19"
  },
  "eslintConfig": {
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "prettier",
      "next/core-web-vitals",
      "plugin:@typescript-eslint/recommended"
    ]
  }
}
