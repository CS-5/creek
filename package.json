{
  "name": "creek",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "npm run next:build && npm run next:export",
    "dev": "npm run backend",
    "next:dev": "next dev",
    "next:build": "next build",
    "next:export": "next export",
    "next:start": "next start",
    "next:lint": "next lint",
    "backend": "dotenv -c -- bash -c 'wrangler pages dev --binding AUTH0_FRONTEND_AUDIENCE=\"$WORKER_AUTH0_FRONTEND_AUDIENCE\" --binding AUTH0_DOMAIN=\"$WORKER_AUTH0_DOMAIN\" --binding AUTH0_BACKEND_SECRET=\"$WORKER_AUTH0_BACKEND_SECRET\" --binding AUTH0_BACKEND_ID=\"$WORKER_AUTH0_BACKEND_ID\" -- npm run next:dev'",
    "frontend": "npm run next:dev"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.9.0",
    "@cloudflare/stream-react": "^1.6.1",
    "daisyui": "^2.13.4",
    "next": "12.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@cfworker/jwt": "^1.12.2",
    "@tailwindcss/typography": "^0.5.2",
    "@cloudflare/workers-types": "^3.4.0",
    "@types/auth0": "^2.34.15",
    "@types/node": "17.0.23",
    "@types/react": "17.0.43",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.0.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "8.12.0",
    "eslint-config-next": "12.1.1",
    "eslint-config-prettier": "^8.5.0",
    "install": "^0.13.0",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.23",
    "typescript": "4.6.3",
    "wrangler": "^0.0.24"
  },
  "eslintConfig": {
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "prettier",
      "next/core-web-vitals",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "@next/next/no-img-element": "off"
    }
  }
}
